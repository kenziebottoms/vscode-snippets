{
  "Import React": {
    "prefix": "re",
    "body": "import React from 'react'"
  },
  "useState": {
    "prefix": "use",
    "body": ["const [$1, $2] = useState($3)"]
  },
  "useEffect": {
    "prefix": "eff",
    "body": [
      "useEffect(() => {",
      "  $1",
      "}, [$2])"
    ]
  },
  "Functional Component": {
    "prefix": "fun",
    "body": [
      "import React from 'react'",
      "\nconst $1 = ({",
      "\t$2",
      "}) => {",
      "\t$3",
      "}",
      "\nexport default $1"
    ]
  }
}
