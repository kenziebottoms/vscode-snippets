{
  "Import React": {
    "prefix": "re",
    "body": ["import React from 'react';"]
  },
  "useState": {
    "prefix": "use",
    "body": ["const [$1, $2] = useState($3)"]
  },
  "useEffect": {
    "prefix": "eff",
    "body": [
      "useEffect(() => {",
      "  $1",
      "}, [$2])"
    ]
  }
}
