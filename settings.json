{
  "bracket-pair-colorizer-2.colors": [],
  "bracket-pair-colorizer-2.scopeLineCSS": [
    "borderStyle : solid",
    "borderWidth : 2px",
    "borderColor : {color}",
    "opacity: 0.25"
  ],
  "breadcrumbs.enabled": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.fontFamily": "'Fira Code', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontWeight": "400",
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": {
    "interface": false,
    "keywords": false
  },
  "editor.tabCompletion": "onlySnippets",
  "editor.tabSize": 2,
  "emmet.showExpandedAbbreviation": "never",
  "explorer.confirmDelete": false,
  "extensions.ignoreRecommendations": true,
  "files.exclude": {
    "**/*.code-search": true,
    "**/*.map": true,
    "**/*.min.*": true,
    "**/*.sql": true,
    "**/.classpath": true,
    "**/.elixir_ls": true,
    "**/.factorypath": true,
    "**/.java-version": true,
    "**/.listing": true,
    "**/.project": true,
    "**/__snapshots__": true,
    "**/_build": true,
    "**/bower_components": true,
    "**/node_modules/": true,
    ".settings/": true,
    "awslogs.conf": true,
    "coverage": true
  },
  "git.autofetch": true,
  "gitlens.advanced.messages": {
    "suppressCreatePullRequestPrompt": true
  },
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "gitlens.sortBranchesBy": "name:asc",
  "gitlens.statusBar.enabled": false,
  "gitlens.views.branches.branches.layout": "list",
  "gitlens.views.formats.commits.label": "${message}",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "search.exclude": {
    "**/*.html": true,
    "**/*.info": true,
    "**/*.lock": true,
    "**/*.map.*": true,
    "**/*.patch": true,
    "**/*.snap": true,
    "**/*.svg": true,
    "**/node_modules/": true,
    "coverage": true,
    "dist": true,
    "package-lock.json": true,
    "package.json": true,
    "public": true,
    "report": true,
    "__mocks__": true
  },
  "todo.colors.types": {
    "BUG": "#e180b8",
    "FIX": "#e180b8",
    "SUS": "#ffc999",
    "TODO": "#0db39d"
  },
  "todo.embedded.exclude": [
    "package*",
    "node_modules",
    "coverage",
    "README.md",
    "report",
    "*.map"
  ],
  "todo.embedded.regex": "(?:<!-- *)?(?:#|// @|//|/\\*+|<!--|--|\\* @|\\{!|\\{\\{!--|\\{\\{!) *(TODO|FIXME|FIX|BUG|SUS|UGLY|HACK|NOTE|IDEA|REVIEW|DEBUG|OPTIMIZE)(?:\\s*\\([^)]+\\))?:?(?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|\\{!|\\{\\{!--|\\{\\{!))|((?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|\\{!|\\{\\{!--|\\{\\{!))|(?: +[^@\\n]+)?))",
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.colorTheme": "Replicant",
  "workbench.startupEditor": "newUntitledFile"
}