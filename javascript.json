{
  "empty describe block": {
    "prefix": "des",
    "description": "Jest: Empty describe block",
    "body": [
      "describe('$1', () => {",
      "\t$2",
      "})"
    ]
  },
  "describe block": {
    "prefix": "di",
    "description": "Jest: It block nested in describe block",
    "body": [
      "describe('$1', () => {",
      "\tit('$2', () => {",
      "\t\t$3",
      "\t});",
      "});"
    ]
  },
  "it each": {
    "prefix": "each",
    "description": "Jest: It.each block",
    "body": [
      "it.each([",
      "\t['$1', $2],",
      "])('%s', (name, $3) => {",
      "\t$4",
      "})"
    ]
  },
  "it block": {
    "prefix": "it",
    "description": "Jest: It block",
    "body": ["it('$1', () => {", "\t$2", "});"]
  },
  // work only
  "Error": {
    "prefix": "err",
    "description": "New Error with response",
    "body": [
      "const error = new Error('$1 server error')",
      "error.response = {}"
    ]
  },
  "Spawn provision": {
    "prefix": "spawn",
    "description": "Error handler spawn provision",
    "body": "[spawn.fn(errorHandler)],"
  },
  "Spawn matcher": {
    "prefix": ".spawn",
    "description": "Error handler spawn assertion",
    "body": ".spawn(errorHandler, error)"
  },
  "Mock Date.now": {
    "prefix": "md",
    "description": "Jest: Mock date.now",
    "body": [
      "const oldDateNow = Date.now",
      "beforeAll(() => {",
      "\tDate.now = jest.fn(() => new Date('2020-01-01T12:00:00Z').getTime())",
      "})",
      "afterAll(() => {",
      "\tDate.now = oldDateNow",
      "})"
    ]
  },
  "Cancel saga tests": {
    "prefix": "canc",
    "description": "Saga cancellation test",
    "body": [
      "it('can be cancelled', () => {",
      "\tfunction * sagaCanceller () {",
      "\t\tconst task = yield realFork($1, { payload$2 })",
      "\t\tyield realCancel(task)",
      "\t}",
      "\tconst cancelSpy = jest.fn((task, next) => {",
      "\t\texpect(isSagaTask(task)).toBe(true)",
      "\t\texpect(task.meta.name).toBe('$1')",
      "\t\treturn next()",
      "\t})",
      "\treturn expectSaga(sagaCanceller)",
      "\t\t.provide({ cancel: cancelSpy })",
      "\t\t.not.put.actionType(CONSTANTS.$3)",
      "\t\t.put(wasCancelled())",
      "\t\t.run()",
      "})"
    ]
  }
}
