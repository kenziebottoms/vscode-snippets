{
  "Print to console": {
    "prefix": "cl",
    "body": ["console.log($1);"]
  },
  "Try/catch": {
    "prefix": "tc",
    "body": [
      "try {",
      "\t$1",
      "} catch(err) {",
      "\tconsole.log('Error', err);",
      "}"
    ]
  },
  "return new Promise": {
    "prefix": "rp",
    "body": ["return new Promise((resolve, reject) => {", "\t$1", "});"]
  },
  "empty describe block": {
    "prefix": "des",
    "body": [
      "describe('$1', () => {",
      "\t$2",
      "})"
    ]
  },
  "describe block": {
    "prefix": "di",
    "body": [
      "describe('$1', () => {",
      "\tit('$2', () => {",
      "\t\t$3",
      "\t});",
      "});"
    ]
  },
  "nested describe block": {
    "prefix": "ddi",
    "body": [
      "describe('$1', () => {",
      "\tdescribe('$2', () => {",
      "\t\tit('$3', () => {",
      "\t\t\t$4",
      "\t\t});",
      "\t});",
      "});"
    ]
  },
  "it each": {
    "prefix": "each",
    "body": [
      "it.each([",
      "\t['$1', $2],",
      "])('%s', (name, $3) => {",
      "\t$4",
      "})"
    ]
  },
  "it block": {
    "prefix": "it",
    "body": ["it('$1', () => {", "\t$2", "});"]
  },
  "Export": {
    "prefix": "ep",
    "body": ["export "]
  },
  "target=blank": {
    "prefix": "tar",
    "body": ["target='_blank'"]
  }
}
