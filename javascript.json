{
  "empty describe block": {
    "prefix": "des",
    "body": [
      "describe('$1', () => {",
      "\t$2",
      "})"
    ]
  },
  "describe block": {
    "prefix": "di",
    "body": [
      "describe('$1', () => {",
      "\tit('$2', () => {",
      "\t\t$3",
      "\t});",
      "});"
    ]
  },
  "nested describe block": {
    "prefix": "ddi",
    "body": [
      "describe('$1', () => {",
      "\tdescribe('$2', () => {",
      "\t\tit('$3', () => {",
      "\t\t\t$4",
      "\t\t});",
      "\t});",
      "});"
    ]
  },
  "it each": {
    "prefix": "each",
    "body": [
      "it.each([",
      "\t['$1', $2],",
      "])('%s', (name, $3) => {",
      "\t$4",
      "})"
    ]
  },
  "it block": {
    "prefix": "it",
    "body": ["it('$1', () => {", "\t$2", "});"]
  },
}
